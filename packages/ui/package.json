{
  "name": "@do-ob/ui",
  "version": "1.1.0",
  "description": "A TailwindCSS and React Aria (Open Source) components library for React projects.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/*.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./index": null,
    "./package.json": "./package.json"
  },
  "keywords": [
    "component",
    "library",
    "react",
    "tailwindcss",
    "typescript",
    "aria"
  ],
  "author": "Eric Crowell <eric@do-ob.io> (https://do-ob.io)",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "watch": "tsc --watch --project tsconfig.build.json"
  },
  "dependencies": {
    "@do-ob/core": "^1.1.0",
    "@heroicons/react": "^2.1.3",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.13",
    "react-aria": "nightly",
    "react-aria-components": "nightly",
    "tailwindcss-animate": "^1.0.7",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-react-aria-components": "^1.1.3"
  },
  "peerDependencies": {
    "framer-motion": "^11.2.10",
    "react": "latest",
    "react-dom": "latest",
    "tailwindcss": "^3.4.3"
  }
}