{
  "name": "@do-ob/ui",
  "version": "1.1.0",
  "description": "A TailwindCSS and NextUI (Open Source) components library for React projects.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/*.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./index": null,
    "./package.json": "./package.json"
  },
  "keywords": [
    "component",
    "library",
    "react",
    "tailwindcss",
    "typescript",
    "nextui"
  ],
  "author": "Eric Crowell <eric@do-ob.io> (https://do-ob.io)",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "watch": "tsc --watch --project tsconfig.build.json"
  },
  "dependencies": {
    "@do-ob/core": "^1.1.0",
    "@heroicons/react": "^2.1.3",
    "react-aria-components": "latest",
    "tailwind-merge": "^2.3.0"
  },
  "peerDependencies": {
    "@nextui-org/react": "^2.4.1",
    "@tailwindcss/typography": "^0.5.13",
    "framer-motion": "^11.2.10",
    "react": "latest",
    "react-dom": "latest",
    "tailwindcss": "^3.4.3"
  }
}